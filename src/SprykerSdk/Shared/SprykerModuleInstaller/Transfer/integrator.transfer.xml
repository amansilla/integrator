<?xml version="1.0"?>
<transfers xmlns="spryker:transfer-01"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="spryker:transfer-01 http://static.spryker.com/transfer-01.xsd">

    <transfer name="ClassInformation">
        <property name="fullyQualifiedClassName" type="string"/>
        <property name="className" type="string"/>
        <property name="filePath" type="string"/>
        <property name="parent" type="ClassInformation"/>
        <property name="classTokenTree" type="array" singular="classToken"/>
        <property name="originalClassTokenTree" type="array" singular="originalClassToken"/>
        <property name="tokens" type="array" singular="token"/>
    </transfer>

    <transfer name="Module">
        <property name="name" type="string"/>
        <property name="nameDashed" type="string"/>
        <property name="organization" type="Organization"/>
    </transfer>

    <transfer name="Organization">
        <property name="name" type="string"/>
        <property name="nameDashed" type="string"/>
    </transfer>

</transfers>
